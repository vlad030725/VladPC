<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:VladPC.ViewModels"
                    xmlns:view="clr-namespace:VladPC.Views.Windows">

    <DataTemplate DataType="{x:Type vm:CatalogViewModel}">
        <view:Catalog/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:CartViewModel}">
        <view:Cart/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:ProfileViewModel}">
        <view:Profile/>
    </DataTemplate>
    
</ResourceDictionary>